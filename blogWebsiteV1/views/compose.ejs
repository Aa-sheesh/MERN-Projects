<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label>Title</label>
            <input type="text" class="form-control" name="titleItem" placeholder="Enter Title">

            <label>Post</label>
            <textarea class="form-control" name="postItem" placeholder="Compose Text" rows="5"></textarea>
        </div>
        <button class="btn btn-primary" type="submit">Publish</button>


    </form>
    <%- include("partials/footer"); -%>